{
  "name": "codegen-cli",
  "version": "1.0.1",
  "description": "A code generator cli tool for front end developers, without configuring Java environments.",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && npm link",
    "test": "jest"
  },
  "bin": {
    "cg": "./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryancui-/codegen-cli.git"
  },
  "keywords": [
    "code-generator",
    "cli"
  ],
  "author": "ryancui-",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryancui-/codegen-cli/issues"
  },
  "homepage": "https://github.com/ryancui-/codegen-cli#readme",
  "dependencies": {
    "art-template": "^4.12.2",
    "chalk": "^2.3.0",
    "commander": "^2.12.2",
    "download-git-repo": "^1.0.1",
    "inquirer": "^4.0.1",
    "mysql": "^2.15.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.9",
    "@types/node": "^8.5.1",
    "jest": "^22.0.3",
    "ts-jest": "^22.0.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
